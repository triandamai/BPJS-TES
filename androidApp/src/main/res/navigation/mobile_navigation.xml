<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_splash">
    <fragment
        android:id="@+id/navigation_splash"
        android:name="com.bluehabit.bpjs.android.featureXml.splash.SplashFragment"
        android:label="@string/title_signin"
        tools:layout="@layout/fragment_splash" />
    <fragment
        android:id="@+id/navigation_signin"
        android:name="com.bluehabit.bpjs.android.featureXml.auth.signIn.SignInFragment"
        android:label="@string/title_signin"
        tools:layout="@layout/fragment_signin" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bluehabit.bpjs.android.featureXml.dashboard.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.bluehabit.bpjs.android.featureXml.dashboard.news.NewsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_digital_card"
        android:name="com.bluehabit.bpjs.android.featureXml.dashboard.digitalCard.DigitalCardFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_digital_card" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.bluehabit.bpjs.android.featureXml.dashboard.profile.ProfileFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_information"
        android:name="com.bluehabit.bpjs.android.featureXml.programInformation.ProgramInformationFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_information" />


    <fragment
        android:id="@+id/navigation_detail_program"
        android:name="com.bluehabit.bpjs.android.featureXml.detailProgram.DetailProgramFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_detail_program" >
        <argument
            android:name="ID"
            app:argType="integer"
            android:defaultValue="0"/>
    </fragment>
</navigation>