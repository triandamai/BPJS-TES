<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_signin">
    <fragment
        android:id="@+id/navigation_signin"
        android:name="com.bluehabit.bpjs.android.featureXml.auth.signIn.SignInFragment"
        android:label="@string/title_signin"
        tools:layout="@layout/fragment_signin" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bluehabit.bpjs.android.featureXml.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.bluehabit.bpjs.android.featureXml.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.bluehabit.bpjs.android.featureXml.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>